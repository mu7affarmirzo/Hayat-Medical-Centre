server {

    server_name lis.dev-hayat.uz www.lis.dev-hayat.uz;

    # Assuming /home/react-app/build is where your React app build directory is
    root /var/www/lis;

    index index.html;

    location / {
        try_files $uri /index.html;
    }

    listen [::]:443 ssl; # managed by Certbot
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/lis.dev-hayat.uz/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/lis.dev-hayat.uz/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
