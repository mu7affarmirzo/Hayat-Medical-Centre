server {
    server_name 185.196.213.3 back-telelon.uz;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/Hayat-Medical-Centre/backend;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/back-telelon.uz/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/back-telelon.uz/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = back-telelon.uz) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name 185.196.213.3 back-telelon.uz;
    return 404; # managed by Certbot
}

